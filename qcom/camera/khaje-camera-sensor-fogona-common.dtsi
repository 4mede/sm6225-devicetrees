/*
   This base is original from "khaje-camera-sensor-idp.dtsi"
*/
#include <dt-bindings/clock/qcom,gcc-khaje.h>
#include <dt-bindings/msm-camera.h>
#include "khaje-camera-flash-pwm.dtsi"

&tlmm {
  cam_sensor_mclk0_active: cam_sensor_mclk0_active {
    mux {
      pins = "gpio20";
      function = "cam_mclk";
    };

    config {
      pins = "gpio20";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

  cam_sensor_mclk0_suspend: cam_sensor_mclk0_suspend {
    mux {
      pins = "gpio20";
      function = "cam_mclk";
    };

    config {
      pins = "gpio20";
      bias-pull-down; /* PULL DOWN */
      drive-strength = <2>; /* 2 MA */
    };
  };

  cam_sensor_mclk1_active: cam_sensor_mclk1_active {
    mux {
      pins = "gpio21";
      function = "cam_mclk";
    };

    config {
      pins = "gpio21";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

  cam_sensor_mclk1_suspend: cam_sensor_mclk1_suspend {
    mux {
      pins = "gpio21";
      function = "cam_mclk";
    };

    config {
      pins = "gpio21";
      bias-pull-down; /* PULL DOWN */
      drive-strength = <2>; /* 2 MA */
    };
  };

  cam_sensor_rear0_reset_active: cam_sensor_rear0_reset_active {
    mux {
      pins = "gpio18";
      function = "gpio";
    };

    config {
      pins = "gpio18";
      bias-disable; /* No PULL */
      drive-strength = <2>;
    };
  };

  cam_sensor_rear0_reset_suspend: cam_sensor_rear0_reset_suspend {
    mux {
      pins = "gpio18";
      function = "gpio";
    };

    config {
      pins = "gpio18";
      bias-pull-down; /* PULL DOWN */
      drive-strength = <2>;
      output-low;
    };
  };

  cam_sensor_front_reset_active: cam_sensor_front_reset_active {
    mux {
      pins = "gpio19";
      function = "gpio";
    };

    config {
      pins = "gpio19";
      bias-disable; /* No PULL */
      drive-strength = <2>;
    };
  };

  cam_sensor_front_reset_suspend: cam_sensor_front_reset_suspend {
    mux {
      pins = "gpio19";
      function = "gpio";
    };

    config {
      pins = "gpio19";
      bias-pull-down; /* PULL DOWN */
      drive-strength = <2>; /* 2 MA */
      output-low;
    };
  };

/* s5kjns s54h7 iovdd enable */
  camera_ldo_iovdd_en: camera_ldo_iovdd_en {
    mux {
      pins = "gpio25";
      function = "gpio";
    };

    config {
      pins = "gpio25";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

/* s5kjns avdd enable */
  camera_ldo_50m_avdd_en: camera_ldo_50m_avdd_en {
    mux {
      pins = "gpio36";
      function = "gpio";
    };

    config {
      pins = "gpio36";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

/* s5k4h7 avdd enable */
  camera_ldo_8m_avdd_en: camera_ldo_8m_avdd_en {
    mux {
      pins = "gpio68";
      function = "gpio";
    };

    config {
      pins = "gpio68";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

/* s5kjns dvdd enable */
  camera_ldo_50m_dvdd_en: camera_ldo_50m_dvdd_en {
    mux {
      pins = "gpio65";
      function = "gpio";
    };

    config {
      pins = "gpio65";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

/* s5kjns afvdd enable */
  camera_ldo_50m_afvdd_en: camera_ldo_50m_afvdd_en {
    mux {
      pins = "gpio26";
      function = "gpio";
    };

    config {
      pins = "gpio26";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

  flash_active: flash_active {
    /* FLASH */
    mux {
      pins = "gpio85";
      function = "gpio";
    };

    config {
      pins = "gpio85";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

  flash_suspend: cam_flash_suspend {
    /* FLASH */
    mux {
      pins = "gpio85";
      function = "gpio";
    };

    config {
      pins = "gpio85";
      bias-pull-down; /* PULL DOWN */
      drive-strength = <2>; /* 2 MA */
      output-low;
    };
  };

  cam_res_mgr_default: cam_res_mgr_default {
    /* FLASH */
    mux {
      pins = "gpio85";
      function = "gpio";
    };

    config {
      pins = "gpio85";
      bias-disable; /* No PULL */
      drive-strength = <2>; /* 2 MA */
    };
  };

  cam_res_mgr_suspend: cam_res_mgr_suspend {
    /* FLASH */
    mux {
      pins = "gpio85";
      function = "gpio";
    };

    config {
      pins = "gpio85";
      bias-pull-down; /* PULL DOWN */
      drive-strength = <2>; /* 2 MA */
      output-low;
    };
  };
};

&pm6125_pwm {
    status = "ok";
};

/* s5k4h7 dvdd enable */
&pm6125_gpios {
  camera_ldo_8m_dvdd_en: camera_ldo_8m_dvdd_en {
    pins = "gpio1";
    function = "normal";
    input-disable;
    output-enable;
    bias-pull-down; /* PULL DOWN */
    power-source = <0>; /* 1.2V */
    qcom,drive-strength = <2>; /* Medium */
  };
};

&soc {
  qcom,cam-res-mgr {
    compatible = "qcom,cam-res-mgr";
    status = "ok";
    gpios = <&tlmm 85 0>;
    pinctrl-names = "cam_res_mgr_default", "cam_res_mgr_suspend";
    pinctrl-0 = <&cam_res_mgr_default>;
    pinctrl-1 = <&cam_res_mgr_suspend>;
  };

  camera_ldo_iovdd: gpio-regulator@0 {
    compatible = "regulator-fixed";
    reg = <0x00 0x00>;
    regulator-name = "camera_ldo_iovdd";
    regulator-min-microvolt = <1800000>;
    regulator-max-microvolt = <1800000>;
    regulator-enable-ramp-delay = <233>;
    enable-active-high;
    gpio = <&tlmm 25 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&camera_ldo_iovdd_en>;
  };

  /*s5kjns avdd*/
  camera_ldo_50m_avdd: gpio-regulator@1 {
    compatible = "regulator-fixed";
    reg = <0x01 0x00>;
    regulator-name = "camera_ldo_50m_avdd";
    regulator-min-microvolt = <2800000>;
    regulator-max-microvolt = <2800000>;
    regulator-enable-ramp-delay = <233>;
    enable-active-high;
    gpio = <&tlmm 36 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&camera_ldo_50m_avdd_en>;
  };

  /*s5kjns dvdd*/
  camera_ldo_50m_dvdd: gpio-regulator@2 {
    compatible = "regulator-fixed";
    reg = <0x02 0x00>;
    regulator-name = "camera_ldo_50m_dvdd";
    regulator-min-microvolt = <1050000>;
    regulator-max-microvolt = <1050000>;
    regulator-enable-ramp-delay = <233>;
    enable-active-high;
    gpio = <&tlmm 65 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&camera_ldo_50m_dvdd_en>;
    vin-supply = <&S7A>;
  };

  /*s5k4h7 avdd*/
  camera_ldo_8m_avdd: gpio-regulator@3 {
    compatible = "regulator-fixed";
    reg = <0x03 0x00>;
    regulator-name = "camera_ldo_8m_avdd";
    regulator-min-microvolt = <2800000>;
    regulator-max-microvolt = <2800000>;
    regulator-enable-ramp-delay = <233>;
    enable-active-high;
    gpio = <&tlmm 68 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&camera_ldo_8m_avdd_en>;
  };

  /*s5k4h7 dvdd*/
  camera_ldo_8m_dvdd: gpio-regulator@4 {
    compatible = "regulator-fixed";
    reg = <0x04 0x00>;
    regulator-name = "camera_ldo_8m_dvdd";
    regulator-min-microvolt = <1200000>;
    regulator-max-microvolt = <1200000>;
    regulator-enable-ramp-delay = <233>;
    enable-active-high;
    gpio = <&pm6125_gpios 1 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&camera_ldo_8m_dvdd_en>;
    vin-supply = <&S7A>;
  };

  /*s5kjns afvdd*/
  camera_ldo_50m_afvdd: gpio-regulator@5 {
    compatible = "regulator-fixed";
    reg = <0x05 0x00>;
    regulator-name = "camera_ldo_50m_afvdd";
    regulator-min-microvolt = <2800000>;
    regulator-max-microvolt = <2800000>;
    regulator-enable-ramp-delay = <233>;
    enable-active-high;
    gpio = <&tlmm 26 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&camera_ldo_50m_afvdd_en>;
  };
};

&cam_cci0 {
  /* s5kjns AF */
  actuator_rear_main: qcom,actuator@0{
    cell-index = <0>;
    compatible = "qcom,actuator";
    cci-master = <0>;
    cam_vaf-supply = <&camera_ldo_50m_afvdd>;
    regulator-names = "cam_vaf";
    rgltr-cntrl-support;
    rgltr-min-voltage = <2800000>;
    rgltr-max-voltage = <2800000>;
    rgltr-load-current = <103000>;
    status = "ok";
  };

  /* pwm  flash fogona*/
  led_flash_rear: qcom,camera-flash@0 {
    cell-index = <0>;
    compatible = "qcom,camera-flash";
    flash-type = <CAM_FLASH_TYPE_GPIO>;
    gpio-no-mux = <0>;
    pinctrl-names = "cam_default", "cam_suspend";
    pinctrl-0 = <&flash_active>;
    pinctrl-1 = <&flash_suspend>;
    gpios = <&tlmm 85 0>;
    gpio-custom1 = <0>;
    gpio-req-tbl-num = <0>;
    gpio-req-tbl-flags = <0>;
    gpio-req-tbl-label = "CUSTOM_GPIO1";
    status = "ok";
  };

  /* Rear s5kjns eeprom */
  eeprom_rear: qcom,eeprom@0 {
    cell-index = <0>;
    reg = <0x0>;
    compatible = "qcom,eeprom";
    cam_vio-supply = <&camera_ldo_iovdd>;
    regulator-names = "cam_vio";
    rgltr-cntrl-support;
    rgltr-min-voltage = <1800000>;
    rgltr-max-voltage = <1800000>;
    rgltr-load-current = <120000>;
    cci-master = <0>;
    status = "ok";
  };

  /* Front s5k4h7 otp */
  eeprom_front: qcom,eeprom@1 {
    cell-index = <1>;
    reg = <0x1>;
    compatible = "qcom,eeprom";
    cam_vio-supply = <&camera_ldo_iovdd>;
    cam_vana-supply = <&camera_ldo_8m_avdd>;
    cam_vdig-supply = <&camera_ldo_8m_dvdd>;
    cam_clk-supply = <&gcc_camss_top_gdsc>;
    regulator-names = "cam_vio", "cam_vana", "cam_vdig", "cam_clk";
    rgltr-cntrl-support;
    rgltr-min-voltage = <1800000 2800000 1200000 0>;
    rgltr-max-voltage = <1800000 2800000 1200000 0>;
    rgltr-load-current = <120000 80000 1200000 0>;
    gpio-no-mux = <0>;
    pinctrl-names = "cam_default", "cam_suspend";
    pinctrl-0 = <&cam_sensor_mclk1_active &cam_sensor_front_reset_active>;
    pinctrl-1 = <&cam_sensor_mclk1_suspend &cam_sensor_front_reset_suspend>;
    gpios = <&tlmm 21 0>, <&tlmm 19 0>;
    gpio-reset = <1>;
    gpio-req-tbl-num = <0 1 >;
    gpio-req-tbl-flags = <1 0 >;
    gpio-req-tbl-label = "CAMIF_MCLK1", "CAM_RESET1";
    cci-master = <1>;
    status = "ok";
    clocks = <&gcc GCC_CAMSS_MCLK1_CLK>;
    clock-names = "cam_clk";
    clock-cntl-level = "turbo";
    clock-rates = <24000000>;
  };

  /* Rear s5kjns */
  rear_main: qcom,cam-sensor@0 {
    cell-index = <0>;
    compatible = "qcom,cam-sensor";
    csiphy-sd-index = <0>;
    sensor-position-roll = <90>;
    sensor-position-pitch = <0>;
    sensor-position-yaw = <180>;
    eeprom-src = <&eeprom_rear>;
    led-flash-src = <&led_flash_rear>;
    actuator-src = <&actuator_rear_main>;
    cam_vio-supply = <&camera_ldo_iovdd>;
    cam_vana-supply = <&camera_ldo_50m_avdd>;
    cam_vdig-supply = <&camera_ldo_50m_dvdd>;
    cam_clk-supply = <&gcc_camss_top_gdsc>;
    regulator-names ="cam_vio", "cam_vana", "cam_vdig", "cam_clk";
    rgltr-cntrl-support;
    rgltr-min-voltage = <1800000 2800000 1050000 0>;
    rgltr-max-voltage = <1800000 2800000 1050000 0>;
    rgltr-load-current = <120000 80000 1200000 0>;
    gpio-no-mux = <0>;
    pinctrl-names = "cam_default", "cam_suspend";
    pinctrl-0 = <&cam_sensor_mclk0_active &cam_sensor_rear0_reset_active>;
    pinctrl-1 = <&cam_sensor_mclk0_suspend &cam_sensor_rear0_reset_suspend>;
    gpios = <&tlmm 20 0>, <&tlmm 18 0>;
    gpio-reset = <1>;
    gpio-req-tbl-num = <0 1>;
    gpio-req-tbl-flags = <1 0>;
    gpio-req-tbl-label = "CAMIF_MCLK0", "CAM_RESET0";
    cci-master = <0>;
    status = "ok";
    clocks = <&gcc GCC_CAMSS_MCLK0_CLK>;
    clock-names = "cam_clk";
    clock-cntl-level = "turbo";
    clock-rates = <24000000>;
  };

  /* Front s5k4h7 */
  front_main: qcom,cam-sensor@1 {
    cell-index = <1>;
    compatible = "qcom,cam-sensor";
    csiphy-sd-index = <2>;
    sensor-position-roll = <270>;
    sensor-position-pitch = <0>;
    sensor-position-yaw = <0>;
    eeprom-src = <&eeprom_front>;
    cam_vio-supply = <&camera_ldo_iovdd>;
    cam_vana-supply = <&camera_ldo_8m_avdd>;
    cam_vdig-supply = <&camera_ldo_8m_dvdd>;
    cam_clk-supply = <&gcc_camss_top_gdsc>;
    regulator-names = "cam_vio", "cam_vana", "cam_vdig", "cam_clk";
    rgltr-cntrl-support;
    rgltr-min-voltage = <1800000 2800000 1200000 0>;
    rgltr-max-voltage = <1800000 2800000 1200000 0>;
    rgltr-load-current = <120000 80000 1200000 0>;
    gpio-no-mux = <0>;
    pinctrl-names = "cam_default", "cam_suspend";
    pinctrl-0 = <&cam_sensor_mclk1_active &cam_sensor_front_reset_active>;
    pinctrl-1 = <&cam_sensor_mclk1_suspend &cam_sensor_front_reset_suspend>;
    gpios = <&tlmm 21 0>, <&tlmm 19 0>;
    gpio-reset = <1>;
    gpio-req-tbl-num = <0 1>;
    gpio-req-tbl-flags = <1 0>;
    gpio-req-tbl-label = "CAMIF_MCLK1", "CAM_RESET1";
    cci-master = <1>;
    status = "ok";
    clocks = <&gcc GCC_CAMSS_MCLK1_CLK>;
    clock-names = "cam_clk";
    clock-cntl-level = "turbo";
    clock-rates = <24000000>;
  };
};
